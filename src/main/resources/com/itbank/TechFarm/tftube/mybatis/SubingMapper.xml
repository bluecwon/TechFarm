<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  

<mapper namespace="com.itbank.TechFarm.tftube.mybatis.SubingMapper">

<insert id="insertSubing" parameterType="subingDTO">
insert into tftube_subing(channel,member_no,subing_member) values(#{channel},#{member_no},#{subing_member})  
</insert>

<select id="get_subing_member" parameterType="int" resultType="int">
select subing_member_no from tftube_subing where member_no=#{member_no}
</select>

<select id="get_subing" parameterType="int" resultType="int">
select * from tftube_subing where member_no=#{member_no}
</select>

</mapper>