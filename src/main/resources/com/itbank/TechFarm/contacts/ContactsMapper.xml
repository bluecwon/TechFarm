<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  

<mapper namespace="com.itbank.techfarm.contacts.MemberMapper">
	<select id = "listContacts" resultType = "contactsDTO">
		select * from tfcontacts
	</select>
  
  	<insert id="insertMember" parameterType="MemberDTO" useGeneratedKeys="true">
	    insert into member(id,passwd,name,email,birthday_year,birthday_month,birthday_day,sex,joindate,nickname)
	     values(
	   		#{id},
	   		#{passwd},
	   		#{name},
	   		#{email},
	   		#{birthday_year},
	   		#{birthday_month},
	   		#{birthday_day},
	   		#{sex},
	   		sysdate(),
	   		default);
	</insert>
  
</mapper>